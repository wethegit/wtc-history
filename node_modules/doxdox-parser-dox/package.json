{
  "name": "doxdox-parser-dox",
  "description": "Dox parser plugin for doxdox.",
  "version": "2.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=8.0"
  },
  "license": "MIT",
  "dependencies": {
